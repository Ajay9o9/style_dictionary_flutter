{
  "source": ["tokens/font.json"],
  "platforms": {
    "flutter": {
      "transforms": ["attribute/cti", "name/cti/camel", "color/hex8flutter", "size/flutter/remToDouble", "content/flutter/literal", "asset/flutter/literal", "font/flutter/literal","app/fonts"],
      "buildPath": "lib/generated/fonts/",
      "files": [
        {
          "destination": "fonts.g.dart",
          "format": "flutter/class.dart",
          "className": "AppFonts",
          "type": "font",
          "filter": {
              "attributes": {
                  "category": "font"
              }
          }
      }
      ],
      "options": {
        "outputReferences": true
      }
  }
  }
}
